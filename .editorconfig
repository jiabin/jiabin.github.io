; EditorConfig is awesome: http://EditorConfig.org
root = true

[*]
indent_style = space
end_of_line = LF
charset = utf-8
trim_trailing_whitespace = true

[*.html]
indent_size = 2

[*.jade]
indent_size = 2

[*.js]
indent_size = 2

[*.css]
indent_size = 2

[*.less]
indent_size = 2

[*.rst]
indent_size = 4
